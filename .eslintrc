{
  parser: "babel-eslint",
  extends: "airbnb",
  env: {
    es6: true,
    mocha: true,
    browser: true
  },
  parserOptions: {
    sourceType: "module"
  },
  settings: {
    "import/resolver": "webpack"
  },
  rules: {
    "class-methods-use-this": 0,
    "react/forbid-prop-types": 0,
    "no-console": 0,
    "react/require-default-props": 0,
    "quotes": 0,
    "object-curly-newline":0,
    "react/prop-types":0,
    "array-callback-return":0,
    "comma-dangle":0,
     "arrow-body-style":0
  }
}
